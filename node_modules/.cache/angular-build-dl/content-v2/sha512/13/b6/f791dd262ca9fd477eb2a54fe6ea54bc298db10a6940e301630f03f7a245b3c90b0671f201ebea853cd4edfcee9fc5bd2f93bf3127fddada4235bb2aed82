       }\n\n    }\n\n    ngOnInit() { }\n\n    selectAllRows(ev) {\n        if (ev) {\n            this.records.forEach(el => {\n                el.isSelected = true;\n            })\n        }\n        else {\n            this.records.forEach(el => {\n                el.isSelected = false;\n            })\n        }\n    }\n\n\n    getSortedData(ev) {\n        if (this.sortingEnabled) {\n            this.sortedBy = ev;\n            this.sortData.emit(ev);\n        }\n    }\n\n\n    getCaretVisiblity(e): boolean {\n        if (this.sortingEnabled && this.sortedBy == e) {\n            return true;\n        }\n\n        else {\n            return false;\n        }\n\n    }\n    getColor(key, data) {\n\n        if (data == \"Absent\") {\n            return 'red'\n\n        }\n        else if (data == \"Leave\") {\n            return 'blue'\n        }\n\n    }\n\n}","export default \".button {\\n  padding: 3px 5px;\\n  background: #fff;\\n  border: 1px solid #ccc;\\n  display: inline-block;\\n  font-size: 14px;\\n  font-weight: 600;\\n  box-sizing: border-box;\\n  cursor: pointer;\\n  color: #0084f6;\\n}\\n\\ntable {\\n  width: 100%;\\n  min-height: 58vh;\\n}\\n\\nth,\\ntd {\\n  text-align: left;\\n  padding: 5px;\\n  word-break: break-word;\\n}\\n\\n.illustration-container {\\n  display: block;\\n}\\n\\n.illustration-container .illustration-img {\\n  width: 40%;\\n  margin-left: 25%;\\n  height: 54vh;\\n}\\n\\ntr:nth-child(even) {\\n  background-color: white;\\n}\\n\\ntr:nth-child(odd) {\\n  background-color: white;\\n}\\n\\nth {\\n  background-color: #e0eaec;\\n  color: black;\\n}\\n\\ni, th {\\n  cursor: pointer;\\n}\\n\\n.backC {\\n  color: red;\\n}\\n\\n.dropup-content {\\n  display: none;\\n  position: absolute;\\n  background-color: #f1f1f1;\\n  min-width: 100px;\\n  bottom: 5px;\\n  z-index: 1;\\n}\\n\\n.dropup-content a {\\n  color: black;\\n  padding: 5px 10px;\\n  text-decoration: none;\\n  display: block;\\n}\\n\\n.dropup-content a:hover {\\n  background-col