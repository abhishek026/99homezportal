;\\n  width: 50%;\\n  position: relative;\\n}\\n\\n.timeline ul:after {\\n  content: '';\\n  width: 55%;\\n  height: 124%;\\n  position: absolute;\\n  left: -2px;\\n  top: -5px;\\n  /* transition: all 0.3s ease-in-out; */\\n  background: #5ad5e8;\\n  border-radius: 100px;\\n}\\n\\n.timeline ul.timelineActive:after {\\n  /* left: -2px; */\\n  -webkit-animation: tabAnimationReverse 0.6s ease-in-out forwards;\\n          animation: tabAnimationReverse 0.6s ease-in-out forwards;\\n}\\n\\n.timeline ul.chatActive:after {\\n  /* left: 47%; */\\n  -webkit-animation: tabAnimation 0.6s ease-in-out forwards;\\n          animation: tabAnimation 0.6s ease-in-out forwards;\\n}\\n\\n.timeline ul.chatActive .chat-tab a,\\n.timeline ul.timelineActive .timeline-tab a {\\n  color: #fff;\\n}\\n\\n.activity-right-sec {\\n  padding: 24px 10px 24px 24px;\\n  max-height: 390px;\\n  overflow: auto;\\n}\\n\\n.activity-right-sec h3 {\\n  color: #414141;\\n  margin-bottom: 10px;\\n}\\n\\n.activity-right-sec-bottom {\\n  border-top: 2px solid rgba(151, 151, 151, 0.5);\\n  margin-top: 10px;\\n  padding: 14px 10px 10px 25px;\\n}\\n\\n.activity-right-sec-bottom .todo-top {\\n  padding-bottom: 15px;\\n}\\n\\n.activity-right-sec-bottom .todo-top h3 {\\n  padding-top: 3px;\\n}\\n\\n.activity-right-sec-bottom .todo-top span {\\n  float: right;\\n  font-size: 14px;\\n  color: #666;\\n  padding-top: 2px;\\n}\\n\\n.activity-right-sec-bottom .todo-top span dfn {\\n  font-style: normal;\\n}\\n\\n.activity-right-sec-bottom .todo-top span dfn,\\n.activity-right-sec-bottom .todo-top span svg {\\n  display: inline-block;\\n  vertical-align: middle;\\n}\\n\\n.todo-tab {\\n  margin: 0 -20px;\\n  border-bottom: 2px solid rgba(151, 151, 151, 0.1);\\n  padding: 10px 20px 0px;\\n  margin-bottom: 15px;\\n}\\n\\n.todo-tab li {\\n  float: left;\\n  width: 50%;\\n  text-align: center;\\n  font-size: 12px;\\n  color: #414141;\\n  font-weight: 600;\\n  cursor: pointer;\\n}\\n\\n.todo-tab li span {\\n  display: inline-bloc