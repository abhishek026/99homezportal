};

let FilterPipe = /** @class */ (() => {
    let FilterPipe = class FilterPipe {
        transform(value, args) {
            if (!value)
                return null;
            if (!args)
                return value;
            args = args.toLowerCase();
            return value.filter((item) => {
                return JSON.stringify(item).toLowerCase().includes(args);
            });
        }
    };
